end clear memorycal 1959 1 4allocate 2001:4open data complex_q.ratdata(for=rat)compute maketable = 1* Estimate Granger causality regressions with 4 lags* First test whether MIC causes SPFlinreg(robusterrors,damp=1,lags=4) adlcpispfa / adlcpispfa_r# constant adlcpispfa{1 to 4} adlcpimica{1 to 4}exclude# adlcpimica{1 to 4}* There is some serial correlation in the residuals, which* is accounted for by the 'robusterrors' argumentcorrelate(qstats) adlcpispfa_r* Note that statistical significance calculations from the * summarize command are incorrect because of the serial correlationsummarize# adlcpimica{1 to 4}* Now test whether SPF causes MIClinreg(robusterrors,damp=1,lags=4) adlcpimica / adlcpimica_r# constant adlcpispfa{1 to 4} adlcpimica{1 to 4}exclude# adlcpispfa{1 to 4}* Yes indeed - significant at 3 percent levelsummarize# adlcpispfa{1 to 4}* Now do it with 8 lags rather than 4if maketable == 1  source(noecho) granger_start.pgm  change output screenend iflinreg adlcpispfa / adlcpispfa_r# constant adlcpispfa{1 to 8} adlcpimica{1 to 8}exclude# constantcompute constsig = %signifsummarize# adlcpispfa{1 to 8}compute sumadlcpispfa = %SUMLCexclude# adlcpispfa{1 to 8}compute sigadlcpispfa = %signifsummarize# adlcpimica{1 to 8}compute sumadlcpimica = %SUMLCexclude# adlcpimica{1 to 8}compute sigadlcpimica = %signif* Again no evidence for MIC -> SPFif maketable == 1  change output fileio  source(noecho) granger_spfOnMic.pgm  change output screenend iflinreg adlcpimica / adlcpimica_r# constant adlcpispfa{1 to 8} adlcpimica{1 to 8}exclude# constantcompute constsig = %signifsummarize# adlcpispfa{1 to 8}compute sumadlcpispfa = %SUMLCexclude# adlcpispfa{1 to 8}compute sigadlcpispfa = %signif* Significant evidence that SPF -> MICsummarize# adlcpimica{1 to 8}compute sumadlcpimica = %SUMLCexclude# adlcpimica{1 to 8}compute sigadlcpimica = %signifif maketable == 1  change output fileio  source(noecho) granger_MicOnSPF.pgm  source(noecho) granger_end.pgm  change output screenend if* Now do Geweke-Meese-Dent version of the test as described* in RATS manual chapter 6 p. 192* First test whether MIC forecasts SPFlinreg adlcpimica / adlcpimica_r# constant adlcpispfa{-4 to 4} adlcpimica{1 to 4}exclude# adlcpispfa{-4 to -1}* No forecasting power at allcorrelate(qstats) adlcpimica_r* No evidence of serial correlationsummarize# adlcpispfa{-4 to -1}* Sum of coefficients is actually negative, makes no sense* Now test whether SPF forecasts MIClinreg adlcpispfa / adlcpispfa_r# constant adlcpispfa{1 to 4} adlcpimica{-4 to 4}exclude# adlcpimica{-4 to -1}* Significant at about the 5 percent levelcorrelate(qstats) adlcpispfa_r* Modest evidence of serial correlationsummarize# adlcpimica{-4 to -1}* Sum of coefficients is positive (though not big)