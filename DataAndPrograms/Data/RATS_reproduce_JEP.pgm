end clear memorycal 1959 1 4allocate 2010:4open data complex_q.ratdata(for=rat)set JEPStudy = 0set pctinfl = 100*(cpi/cpi{4}-1)set pctinfl_tp4 = pctinfl{-4}clear AfterSPFStartssmpl 1981:3 1997:4set JEPStudy = 1   ;* set SPFaerr = adlcpispfa -pctinfl_tp4linreg(noprint) spfaerr# constantset SPFaabserr = abs(adlcpispfa - pctinfl_tp4)linreg(noprint) spfaabserr# constantset spfaerrsq = spfaerr*spfaerraccumulate spfaerrsq / accumspfaerrsqaccumulate JEPstudy / accumjepstudycompute RMSEspfa = (accumspfaerrsq(1997:4)/(accumjepstudy(1997:4)-1))**0.5set micaerr = adlcpimica-pctinfl_tp4linreg(noprint) micaerr # constantset micaabserr = abs(adlcpimica-pctinfl_tp4)linreg(noprint) micaabserr# constantset micaerrsq = micaerr*micaerraccumulate micaerrsq / accummicaerrsqcompute RMSEmica = (accummicaerrsq(1997:4)/(accumjepstudy(1997:4)-1))**0.5set spfmerr = adlcpispfm -pctinfl_tp4linreg(noprint) spfmerr# constantset spfmabserr = abs(adlcpispfm - pctinfl_tp4)linreg(noprint) spfmabserr# constantset spfmerrsq = spfmerr*spfmerraccumulate spfmerrsq / accumspfmerrsqcompute RMSEspfm = (accumspfmerrsq(1997:4)/(accumjepstudy(1997:4)-1))**0.5set micmerr = adlcpimicm-pctinfl_tp4linreg(noprint) micmerr # constantset micmabserr = abs(adlcpimicm-pctinfl_tp4)linreg(noprint) micmabserr# constantset micmerrsq = micmerr*micmerraccumulate micmerrsq / accummicmerrsqcompute RMSEmicm = (accummicmerrsq(1997:4)/(accumjepstudy(1997:4)-1))**0.5disp 'RMSEspfa = ' RMSEspfadisp 'RMSEmica = ' RMSEmicadisp 'RMSEspfm = ' RMSEspfmdisp 'RMSEmicm = ' RMSEmicm