* Mnemonics below: adl is an annual difference in logs; _tpx is an expectation for period t+x* mic is for michigan; m is for median; a is for average;* qdl is a quarterly difference in logs at an annual rateset adlcpimica = michinfset adlcpimicm = michinfmedset adlcpi = 100*(log(cpi)-log(cpi{4})) ;* 4th diff of log qtrly  CPI  = infl over the past yearset adlcor = 100*(log(cor)-log(cor{4})) ;* 4th diff of log qtrly  Core = infl over the past yearset adlcpi_tp4 = adlcpi{-4} ; * Actual outcome corresponding to current period's 4 qtr expectation set adlcor_tp4 = adlcor{-4}* SPF forecast for inflation over the next year is avg of forecasts for next 4 qtrsset adlcpispfm = (qdlcpispfm_tp1+qdlcpispfm_tp2+qdlcpispfm_tp3+qdlcpispfm_tp4)/4set adlcpispfa = (qdlcpispfa_tp1+qdlcpispfa_tp2+qdlcpispfa_tp3+qdlcpispfa_tp4)/4set uspfm      = (uspfm_tp1+uspfm_tp2+uspfm_tp3+uspfm_tp4)/4set uspfa      = (uspfa_tp1+uspfa_tp2+uspfa_tp3+uspfa_tp4)/4set adadlcpimica   = adlcpimica - adlcpimica{4}    ; * Change in expected inflation over the last yearset adadlcpispfm  = adlcpispfm-adlcpispfm{4}  ; * Change in SPF forecasts of inflation over the last yearset adadlcpispfa  = adlcpispfa-adlcpispfa{4}  ; * Change in SPF forecasts of inflation over the last yearset adadlcpi      = adlcpi -  adlcpi{4}           ; * Change in inflation rate over the past year set adadlcor      = adlcor -  adlcor{4}           ; * Change in core inflation rate vs last year'sset adadlcor_tp4  = adadlcor{-4}                  ; * Outcome 4 qtrs from now for change in core inflationset adadlcpi_tp4  = adadlcpi{-4}                  ; * Outcome 4 qtrs from now for change in cpi inflationset adadlcormica  = adlcpimica - adlcor           ; * Predicted change in inflation from Michiganset adadlcorspfm  = adlcpispfm - adlcor           ; * Predicted change in inflation from SPFset adadlcorspfa  = adlcpispfa - adlcor           ; * Predicted change in inflation from SPFset dadlcpimica   = adlcpimica - adlcpimica{1}    ; * Change in Michigan expectationsset dadlcpi       = adlcpi -  adlcpi{1}           ; * Change in 4 quarter inflation rateset dadlcor       = adlcor -  adlcor{1}           ; * Change in 4 quarter core inflation rateset dadlcpispfm = adlcpispfm-adlcpispfm{1}  ; * Change in SPF expectationsset dadlcpispfa = adlcpispfa-adlcpispfa{1}  ; * Change in SPF expectations*   123456789012345set adlcorspfmerr = adlcor{-4}-adlcpispfm       ; * Actual infl over next 4 qtrs minus SPF expset adlcorspfaerr = adlcor{-4}-adlcpispfa       ; * Actual infl over next 4 qtrs minus SPF expset adlcormicerr = adlcor{-4}-adlcpimica       ; * Actual infl over next 4 qtrs minus Mich  expset qdlcor       = 400*(log(cor)-log(cor{1}))set qdlcor_tp4   = qdlcor{-4}set adqdlcor     = qdlcor - qdlcor{4}set adqdlcor_tp4 = adqdlcor{-4}set adqdlcor_tp4 = qdlcor{-4}    -qdlcorset adqdlcorspfm = qdlcpispfm_tp4-qdlcorset adqdlcorspfa = qdlcpispfa_tp4-qdlcorset qdlcpi       = 400*(log(cpi)-log(cpi{1}))set qdlcpi_tp4   = qdlcpi{-4}set adqdlcpi     = qdlcpi - qdlcpi{4}set adqdlcpi_tp4 = adqdlcpi{-4}set adqdlcpi_tp4 = qdlcpi{-4}    -qdlcpiset adqdlcpispfm = qdlcpispfm_tp4-qdlcpiset adqdlcpispfa = qdlcpispfa_tp4-qdlcpiset adqdlcpimica = adlcpimica - qdlcpiset adqdlcormica = adlcpimica - qdlcor